    <body class="text-center">

        <div class="container">

		<!-- <SForm class="CarForm"> -->

<p><label for="id_license_plate">License plate:</label> <input type="text" name="license_plate" maxlength="7" required id="id_license_plate"></p>
<p><label for="id_vin_number">Vin number:</label> <input type="text" name="vin_number" maxlength="17" required id="id_vin_number"></p>
<p><label for="id_purchase_date">Purchase date:</label> <input type="text" name="purchase_date" value="2020-05-18" required id="id_purchase_date"><input type="hidden" name="initial-purchase_date" value="2020-05-18" id="initial-id_purchase_date"></p>
<p><label for="id_miles">Miles:</label> <input type="number" name="miles" value="0" min="0" required id="id_miles"></p>
